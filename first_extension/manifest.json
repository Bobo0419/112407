{
  "name": "GoogleMapsReviewFilter", 
  "version": "1.0",
  "description": "Quick Filter for Fake Reviews",
  "manifest_version": 2,
  "permissions":["activateTab"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts":[
    {
        "matches":["<all_urls>"],
        "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }

}
