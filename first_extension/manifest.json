{
  "name": "GoogleMapsReviewFilter", 
  "version": "1.0",
  "description": "Quick Filter for Fake Reviews",
  "manifest_version": 2,
  "content_scripts":[
    {
        "matches":[
            "<all_urls>"

        ],
        "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action":{
    "default_icon":"icon_16.jpg"
  }
    
}
  